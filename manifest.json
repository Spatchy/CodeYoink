{
  "name": "CodeYoink",
  "description": "Allows one-click copy and paste from any html code block",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.6.0.min.js", "content-script.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action":{
    "default_popup": "popup/index.html"
  },
  "permissions": [
    "storage"
  ]
}